{
    "name": "nuxt-app",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "nuxt build",
        "dev": "nuxt dev --dotenv .env.local",
        "generate": "nuxt generate",
        "preview": "nuxt preview",
        "postinstall": "nuxt prepare",
        "docs:js": "jsdoc -c jsdoc.json",
        "docs:ts": "typedoc --options typedoc.json",
        "docs:copy": "powershell -Command \"Remove-Item -Recurse -Force ./public/dev-docs; Copy-Item -Recurse -Force ./docs ./public/dev-docs\"",
        "docs:generate": "npm run docs:ts && npm run docs:copy"
    },
    "dependencies": {
        "@nuxt/icon": "^1.11.0",
        "@nuxt/image": "^1.10.0",
        "@nuxt/ui": "^3.0.0",
        "@pinia/nuxt": "^0.10.1",
        "@tailwindcss/vite": "^4.0.14",
        "nuxt": "^3.16.0",
        "pinia": "^3.0.1",
        "socket.io-client": "^4.8.1",
        "tailwindcss": "^4.0.14",
        "typescript": "^5.8.2",
        "valibot": "^1.0.0",
        "vue": "^3.5.13",
        "vue-router": "^4.5.0"
    },
    "devDependencies": {
        "@types/express": "^5.0.1",
        "@types/node": "^22.14.0",
        "nuxt-lodash": "^2.5.3",
        "typedoc": "^0.28.2"
    }
}